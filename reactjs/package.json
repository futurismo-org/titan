{
  "name": "titan-client-app",
  "description": "Client App for Titan",
  "private": true,
  "scripts": {
    "dev": "PORT=3000 REACT_APP_ENV=development react-app-rewired start",
    "start": "expo start --tunnel",
    "build": "npm run build:timestamp && GENERATE_SOURCEMAP=true react-app-rewired build",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build:timestamp": "./bin/update-build-timestamp.sh",
    "build:ios": "npm run build:timestamp && expo build:ios",
    "build:android": "npm run build:timestamp && expo build:android",
    "build:web": "npm run build:timestamp && expo build:web",
    "start-sw": "npm run build && http-server ./build",
    "upload-source-maps": "rm -rf sourceMaps && mkdir sourceMaps && mv build/static/js/*.map sourceMaps/ && sh ./bin/upload-script.sh",
    "deploy:netlify": "REACT_APP_ENV=development npm run build && npm run export",
    "deploy:development": "REACT_APP_ENV=development npm run build && firebase deploy -P development",
    "deploy:production": "REACT_APP_GIT_SHA=`git rev-parse HEAD` REACT_APP_ENV=production npm run build && npm run upload-source-maps && firebase deploy -P production",
    "deploy:expo": "npm run build:timestamp && REACT_APP_ENV=production expo publish",
    "login:expo": "expo login",
    "deploy:web:netlify": "netlify deploy --dir web-build",
    "publish:web:netlify": "REACT_APP_ENV=development build:web && deploy:web:netlify",
    "optimize": "expo optimize",
    "deploy:all": "npm run deploy:development && npm run deploy:production",
    "rules:development": "firebase deploy --only firestore:rules -P development",
    "rules:production": "firebase deploy --only firestore:rules -P production",
    "rules:all": "npm run rules:development && npm run rules:production",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --fix --ext .ts,.tsx",
    "watch": "TSC_NONPOLLING_WATCHER=1 npm run tsc -w",
    "sss": "./bin/rg_fzf_remote.sh",
    "ss": "./bin/rg_fzf.sh"
  },
