{
  "name": "titan-client-app",
  "description": "Client App for Titan",
  "private": true,
  "scripts": {
    "dev": "PORT=3000 REACT_APP_ENV=development react-scripts start",
    "build": "react-scripts build",
    "start-sw": "npm run build && http-server ./build",
    "deploy:netlify": "REACT_APP_ENV=development npm run build && npm run export",
    "deploy:development": "REACT_APP_ENV=development npm run build && firebase deploy -P development",
    "deploy:production": "REACT_APP_ENV=production npm run build && firebase deploy -P production",
    "deploy:all": "npm run deploy:development && npm run deploy:production",
    "rules:development": "firebase deploy --only firestore:rules -P development",
    "rules:production": "firebase deploy --only firestore:rules -P production",
    "rules:all": "npm run rules:development && npm run rules:production",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --fix --ext .ts,.tsx",
    "watch": "TSC_NONPOLLING_WATCHER=1 npm run tsc -w",
    "sss": "./bin/rg_fzf_remote.sh",
    "ss": "./bin/rg_fzf.sh"
  },
  "dependencies": {
    "@material-ui/core": "^4.1.2",
    "@material-ui/icons": "^4.0.0",
    "axios": "^0.19.0",
    "connected-react-router": "^6.5.0",
    "discord-rpc": "^3.0.2",
    "dom-to-image": "^2.6.0",
    "dotenv": "^8.0.0",
    "firebase": "^6.1.0",
    "firebase-functions": "^2.3.1",
    "firebaseui": "^4.0.0",
    "form-data": "^2.4.0",
    "history": "^4.9.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-apollo-hooks": "^0.4.5",
    "react-dom": "^16.8.6",
    "react-firebase-hooks": "^2.0.0",
    "react-firebaseui": "^3.1.2",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.0.3",
    "react-redux-firebase": "^2.2.6",
    "react-responsive": "^7.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-share": "^3.0.0",
    "react-stripe-elements": "^3.0.0",
    "recharts": "^1.6.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-firestore": "^0.8.0",
    "redux-thunk": "^2.3.0",
    "remark": "^10.0.1",
    "remark-react": "^5.0.1",
    "rollbar": "^2.7.1",
    "shortid": "^2.2.14",
    "styled-components": "^4.2.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dom-to-image": "^2.6.0",
    "@types/form-data": "^2.2.1",
    "@types/history": "^4.7.2",
    "@types/html2canvas": "^0.0.35",
    "@types/material-ui": "^0.21.6",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.0.10",
    "@types/node-fetch": "^2.3.4",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "^7.0.9",
    "@types/react-responsive": "^3.0.3",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-share": "^2.1.1",
    "@types/react-stripe-elements": "^1.1.10",
    "@types/recharts": "^1.1.16",
    "@types/shortid": "^0.0.29",
    "@types/styled-components": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.14.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "http-server": "^0.11.1",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.1",
    "typescript-styled-plugin": "^0.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
